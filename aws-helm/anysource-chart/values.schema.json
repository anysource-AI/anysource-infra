                },
                "storageClass": {
                  "type": "string"
                }
              }
            }
          }
        },
        "readReplicas": {
          "type": "object",
          "properties": {
            "replicaCount": {
              "type": "integer",
              "minimum": 0
            },
            "resources": {
              "type": "object",
              "properties": {
                "requests": {
                  "type": "object",
                  "properties": {
                    "cpu": {
                      "type": "string",
